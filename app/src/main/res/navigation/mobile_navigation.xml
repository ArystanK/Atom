<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="kz.maxwell.atom.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_shop"
        android:name="kz.maxwell.atom.ui.shop.ShopFragment"
        tools:layout="@layout/fragment_shop" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="kz.maxwell.atom.ui.map.MapFragment"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="kz.maxwell.atom.ui.profile.ProfileFragment"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_advices"
        android:name="kz.maxwell.atom.ui.advices.AdvicesFragment"
        tools:layout="@layout/fragment_advices" />

</navigation>